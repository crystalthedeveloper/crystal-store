		"lucide-react": "0.536.0",
		"ms": "^2.1.3",
		"nanoid": "5.1.5",
		"next": "15.4.6",
		"next-themes": "0.4.6",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"schema-dts": "1.1.5",
		"server-only": "0.0.1",
		"sonner": "2.0.6",
