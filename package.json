{
	"$schema": "https://json.schemastore.org/package",
	"name": "store",
	"version": "1.12.0",
	"license": "AGPL-3.0-only",
	"private": true,
	"type": "module",
	"packageManager": "npm@11.5.2",
	"scripts": {
		"preinstall": "node -e \"const ua=process.env.npm_config_user_agent||''; if(!ua.startsWith('npm/')){ console.error('\\nThis project uses npm.\\nUse: npm install && npm run dev\\n'); process.exit(1);} \" && (command -v corepack >/dev/null 2>&1 && corepack enable >/dev/null 2>&1 && corepack prepare npm@11.5.2 --activate >/dev/null 2>&1 || true)",
		"config:webflow": "node src/scripts/select-config.cjs",
		"dev": "node src/scripts/select-config.cjs && next dev",
		"dev:debug": "NEXT_DEBUG=true node src/scripts/select-config.cjs && next dev",
		"build": "node src/scripts/select-config.cjs && next build",
		"build:debug": "NEXT_DEBUG=true node src/scripts/select-config.cjs && next build",
		"start": "next start",
		"lint": "biome check --write",
		"test": "vitest",
		"docker:build": "docker build -t store .",
		"docker:run": "docker run -d -p 3000:3000 store",
		"clean": "node -e \"try{require('fs').rmSync('node_modules',{recursive:true,force:true});require('fs').rmSync('.next',{recursive:true,force:true});require('fs').rmSync('package-lock.json',{force:true});}catch(e){}\" && npm cache verify"
	},
	"dependencies": {
		"@ai-sdk/openai": "2.0.0",
		"@ai-sdk/react": "2.0.8",
		"@radix-ui/react-checkbox": "1.3.2",
